{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/iligan_flood_hazard.geojson",
      "destination": "/iligan_flood_hazard.geojson.gz"
    },
    {
      "source": "/iligan_landslide_hazard.geojson",
      "destination": "/iligan_landslide_hazard.geojson.gz"
    },
    {
      "source": "/iligan_safdz.geojson",
      "destination": "/iligan_safdz.geojson.gz"
    },
    {
      "source": "/iligan_slope.geojson",
      "destination": "/iligan_slope.geojson.gz"
    },
    {
      "source": "/iligan_landuse.geojson",
      "destination": "/iligan_landuse.geojson.gz"
    },
    {
      "source": "/iligan_ancestral_domain.geojson",
      "destination": "/iligan_ancestral_domain.geojson.gz"
    }
  ],
  "headers": [
    {
      "source": "/(.*).geojson.gz",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/geo+json"
        },
        {
          "key": "Content-Encoding",
          "value": "gzip"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).geojson",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/geo+json"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Accept-Encoding",
          "value": "gzip, deflate, br"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/(.*).(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

